# Example .env file for Hospital Node

# Hospital Configuration
HOSPITAL_ID=hospital_1
HOSPITAL_NAME=General Hospital 1

# Aggregator Connection
# IMPORTANT: Replace with the actual IP address of the aggregator laptop
AGGREGATOR_IP=192.168.1.100
AGGREGATOR_PORT=50051

# Dataset Configuration
DATASET_PATH=/data
DATASET_TYPE=shenzhen  # or 'montgomery'

# Training Configuration
LOCAL_EPOCHS=5
BATCH_SIZE=32
LEARNING_RATE=0.001

# Encryption
ENABLE_ENCRYPTION=true
